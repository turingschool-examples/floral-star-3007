<section id= "attributes">
  <h1><%= @park.name %></h1>
  <h3>Price of Admission: $<%= @park.price_of_admission %>.00</h3>
</section>

<section id='rides'>
  <h3>Rides:</h3>
  <% @park.rides_alpha.each do |ride| %>
    <p>- <%= ride.name %></p>
  <% end %>
</section>

<section id='average'>
  <h3>Average Thrill Rating of Rides: <%= @park.av_rating.round(2) %>/10</h3>
</section>
